{
	"manifest_version": 2,

	"name": "signTextLSF",
	"description": "This extension emulates Netscape's crypto.signText() using LSF.",
	"version": "1.0",

	"content_scripts": [{
		"matches": [
			"*://*/*"
		],
		"js": [
			"contentscript.js"
		]
	}],
	"permissions": [
		"http://127.0.0.1:8090"
	],
	"web_accessible_resources": [
		"script.js"
	]
}
